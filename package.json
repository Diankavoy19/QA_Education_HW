{
  "name": "qa_education_hw",
  "version": "1.0.0",
  "description": "Homework for online training of autotests using <a href=\"https://webdriver.io\">WebdriverIO</a> <br>\r Link to tutorials on [youtube]( https://www.youtube.com/watch?v=cSQxQpIlVgo&list=PLP1-M5OhC7yjAZ1-llDH45nxLfKKbcxGe&index=3)",
  "main": "lessons.spec.js",
  "scripts": {
    "homework1": "npx wdio run ./wdio.conf.js --spec Trainee_Level1_HW_1/Homework1.spec.js",
    "homework2": "npx wdio run ./wdio.conf.js --spec Trainee_Level1_HW_2/Homework2.spec.js",
    "lessons": "npx wdio run ./wdio.conf.js --spec lessons.spec.js",
    "homeworkPOM": "npx wdio run ./wdio.conf.js --spec Trainee_Level8_HW_2/HomeworkwithPageObjects.spec.js",
    "finalHW": "npx wdio run ./wdio.conf.js --spec FinalHomework/*.*"
  },
